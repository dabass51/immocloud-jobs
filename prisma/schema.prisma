datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

enum Status {
  OPEN
  ACCEPTED
  DECLINED
}

model Applicant {
  id     String  @id @default(auto()) @map("_id") @db.ObjectId
  name   String
  status Status @default(OPEN)
}